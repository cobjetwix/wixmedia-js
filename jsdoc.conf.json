{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "opts": {
        "lenient": false
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "systemName"      : "Wix Media",
        "footer"          : "<a href='http://media.wix.com/docs'>Wix Media Developer Center</a>",
        "copyright"       : "2014, Wix",
        "navType"         : "vertical",
        "theme"           : "spacelab",
        "linenums"        : false,
        "collapseSymbols" : false,
        "inverseNav"      : false
    }
}